            </ol>
          </div>
        </div>
      </div><!-- /.container-fluid -->
    </section>
    <!-- /.content-header -->
    <!-- Main content -->
    <section class="content">
        <div class="container-fluid">
          <div class="row">
            <div class="col-12">
              <div class="card">
                <div class="card-header">
                  <h3 class="card-title">Danh sách căn hộ</h3>
                </div>
                <!-- /.card-header -->
                <div class="card-body">
                  <table id="example1" class="table table-bordered table-striped">
                    <thead>
                    <tr>
                      <!-- <th>ID</th> -->
                       <th>STT</th>
                      <th>Tên căn hộ</th>
                      <th>Chủ căn hộ</th>
                      <th>Loại bất động sản</th>
                      <th>Tầng</th>
                      <th>Diện tích</th>
                      <th>Số phòng ngủ</th>
                      <th>Số phòng vệ sinh</th>
                      <th>Giá bán</th>
                      <th>Giá cho thuê</th>
                      <th>Cho thuê từ ngày</th>
                      <th>Cho thuê đến ngày</th>
                      <th>Nội thất</th>
                      <th>Số ban công</th>
                      <th>Hướng ban công</th>
                      <th>Thời gian cập nhật</th>
                      <th>Trạng thái</th>
                      <th>Ghi chú</th>
                    </tr>
                    </thead>
                    <tbody>
                      <% data.forEach((item,index) => { %>
                    <tr>
                      <td><%= index+1 %></td>
                      <td><%= item.apartment_name %></td>
                      <td><%= item.owner.husband_name %></td>
                      <td><%= item.property.property_name %></td>
                      <td><%= item.floor %></td>
                      <td><%= item.area %></td>
                      <td><%= item.bedrooms %></td>
                      <td><%= item.bathrooms %></td>
                      <td><%= item.sale_price %></td>
                      <td><%= item.rental_price %></td>
                      <td ><%= item.available_from %></td>
                      <td ><%= item.available_until %></td>
                      <td><%= item.furnished %></td>
                      <td><%= item.balconies %></td>
                      <td><%= item.balcony_direction.balcony_direction_name %></td>
                      <td ><%= item.last_updated %></td>
                      <td><%= item.status.status_name %></td>
                      <td><%= item.notes %></td>
